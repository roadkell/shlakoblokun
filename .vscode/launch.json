	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: shlakoblokun",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"args": [
				"-i",
				"${workspaceFolder}/shlakoblokun/data/ru/n.txt",
				"-w2",
				"${workspaceFolder}/shlakoblokun/data/ru/adj.txt",
				"${workspaceFolder}/shlakoblokun/data/ru/adv.txt",
				"${workspaceFolder}/shlakoblokun/data/ru/v.txt",
				"-e",
				"${workspaceFolder}/shlakoblokun/data/ru/.exclude-overlaps.txt",
				"-o",
				"${workspaceFolder}/output-ru.txt",
				"-d",
				"3",
				"-r",
				"-u"
			],
			"console": "integratedTerminal",
			"justMyCode": true
		}
	]
}